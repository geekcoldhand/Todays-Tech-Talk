<div class="container">
  <h1 class="mb-4">{{user.username}} Post</h1>

  <div class="form-group">
    <label for="title">Title</label>

    {{post.title}}

  </div>
  <div class="form-group">
    <label for="description">User</label>
    {{post.userId}}
  </div>
  <div class="form-group">
    <label for="markdown">Markdown</label>
    {{post.content}}</textarea>
  </div>

</div>

